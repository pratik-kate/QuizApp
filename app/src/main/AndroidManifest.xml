        android:supportsRtl="true"
        android:theme="@style/Theme.QuizzApp">
        <activity android:name="com.supertridents.learn.digital.marketing.quiz.QuestionActivity"></activity>
        <activity android:name="com.supertridents.learn.digital.marketing.quiz.GameActivity" />
